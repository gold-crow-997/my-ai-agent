      "retryOnFail": true
    },
    {
      "parameters": {
        "jsCode": "// Safely extract the AI output array\nconst results = $json.output || [];\n\n// Check if it's a string (sometimes models return stringified JSON)\nconst data = typeof results === 'string' ? JSON.parse(results) : results;\n\n// Expand into individual n8n items\nreturn data.map(trend => ({ json: trend }));\n"
      },
