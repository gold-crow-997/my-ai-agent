      "parameters": {
        "jsCode": "// Take the first item (NewsAPI response)\nconst articles = items[0].json.articles;\n\n// Clean and structure the data\nreturn articles.map(article => ({\n  json: {\n    title: article.title,\n    description: article.description,\n    source: article.source?.name,\n    url: article.url,\n    publishedAt: article.publishedAt\n  }\n}));"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        624,
        0
      ],
      "id": "b1f5a10b-85d5-40d9-856f-5afd65ec3346",
      "name": "Extract Articles"
    },
    {
      "parameters": {
        "jsCode": "// Combine all items into a readable text block\nconst combinedText = items.map(i => {\n  const a = i.json;\n  return `${a.title}\\n${a.description || ''}\\nSource: ${a.source}\\nDate: ${a.publishedAt}\\nURL: ${a.url}\\n`;\n}).join('\\n\\n');\n\nreturn [{ json: { newsText: combinedText } }];\n\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
